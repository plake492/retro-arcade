
<div class="row">
    {{#each list}}

        <div class="column card">
            <div>{{name}}</div>
            <img src="{{image}}" alt="">
            <div>
                <a href="{{link}}">Play this game</a>
            </div>
            <div class="textbox">
                <p>{{about}}</p>
            </div>
        </div>
    {{/each}}
</div>


